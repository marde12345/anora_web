extends layout

block content
 section.py-5.bg-dark.inner-header
  .container
    .row
      .col-md-12.text-center
        h1.mt-0.mb-3.text-white Telusuri Statistisi
        .breadcrumbs
          p.mb-0.text-white
            a.text-white(href='#') Beranda
            |   /  
            span.text-success Daftar Statistisi

 section.py-5.p-list-two
  .container
    .row
      .col-lg-3
        .filters.shadow-sm.rounded.bg-white.mb-4
          .filters-header.border-bottom.pl-4.pr-4.pt-3.pb-3
            h5.m-0 Cari Berdasarkan 
          .filters-body
            #accordion
              .filters-card.border-bottom.p-4
                #headingOne.filters-card-header
                  h6.mb-0
                    a.btn-link(href='#' data-toggle='collapse' data-target='#collapseOne' aria-expanded='true' aria-controls='collapseOne')
                      | Populer 
                      i.mdi.mdi-chevron-down.float-right
                #collapseOne.collapse.show(aria-labelledby='headingOne' data-parent='#accordion')
                  .filters-card-body.card-shop-filters
                    .custom-control.custom-checkbox
                      input#cb1.custom-control-input(type='checkbox')
                      label.custom-control-label(for='cb1') SPSS
                    .custom-control.custom-checkbox
                      input#cb2.custom-control-input(type='checkbox')
                      label.custom-control-label(for='cb2') Python
                    .custom-control.custom-checkbox
                      input#cb3.custom-control-input(type='checkbox')
                      label.custom-control-label(for='cb3') R
                    .custom-control.custom-checkbox
                      input#cb4.custom-control-input(type='checkbox')
                      label.custom-control-label(for='cb4') Analisis Regresi
                    .custom-control.custom-checkbox
                      input#cb3.custom-control-input(type='checkbox')
                      label.custom-control-label(for='cb3') Olah Data
                    .mt-2
                      a.link(href='#') Lihat semua
              .filters-card.border-bottom.p-4
                #headingTwo.filters-card-header
                  h6.mb-0
                    a.btn-link(href='#' data-toggle='collapse' data-target='#collapsetwo' aria-expanded='true' aria-controls='collapsetwo')
                      | Kategori 
                      i.mdi.mdi-chevron-down.float-right
                #collapsetwo.collapse.show(aria-labelledby='headingTwo' data-parent='#accordion')
                  .filters-card-body.card-shop-filters
                    form.filters-search.mb-3
                      .form-group
                        i.fa.fa-search
                        input.form-control(type='text' placeholder='Cari Kategori')
                    .custom-control.custom-checkbox
                      input#cb6.custom-control-input(type='checkbox')
                      label.custom-control-label(for='cb6') Olah Data
                    .custom-control.custom-checkbox
                      input#cb7.custom-control-input(type='checkbox')
                      label.custom-control-label(for='cb7') Data Entry
                    .custom-control.custom-checkbox
                      input#cb8.custom-control-input(type='checkbox')
                      label.custom-control-label(for='cb8') Jasa Pembuatan Kuisioner
                    .custom-control.custom-checkbox
                      input#cb9.custom-control-input(type='checkbox')
                      label.custom-control-label(for='cb9') Konsultasi Statistik
                    .mt-2
                      a.link(href='#') Lihat semua
              .filters-card.p-4.border-bottom
                #headingOffer.filters-card-header
                  h6.mb-0
                    a.btn-link(href='#' data-toggle='collapse' data-target='#collapseOffer' aria-expanded='true' aria-controls='collapseOffer')
                      | Rating Statistisi 
                      i.mdi.mdi-chevron-down.float-right
                #collapseOffer.collapse(aria-labelledby='headingOffer' data-parent='#accordion')
                  .filters-card-body.card-shop-filters
                    .custom-control.custom-checkbox
                      input#cb6.custom-control-input(type='checkbox')
                      label.custom-control-label(for='cb6')
                        | Top Rated Seller 
                        small.text-danger.float-right 80
                    .custom-control.custom-checkbox
                      input#cb7.custom-control-input(type='checkbox')
                      label.custom-control-label(for='cb7')
                        | Level One 
                        small.text-danger.float-right 34
                    .custom-control.custom-checkbox
                      input#cb8.custom-control-input(type='checkbox')
                      label.custom-control-label(for='cb8')
                        | Level Two 
                        small.text-danger.float-right 56
                    .custom-control.custom-checkbox
                      input#cb9.custom-control-input(type='checkbox')
                      label.custom-control-label(for='cb9')
                        | New Seller 
                        small.text-danger.float-right 98
                    .custom-control.custom-checkbox
                      input#cb10.custom-control-input(type='checkbox')
                      label.custom-control-label(for='cb10')
                        | Seller 
                        small.text-danger.float-right 21
                    .mt-2
                      a.link(href='#') See all
      .col-lg-9.view_slider.recommended
        .row
          .col-lg-12
            .sorting-div.d-flex.align-items-center.justify-content-between
              p.mb-2 463 Statistisi ditemukan
              .sorting.d-flex.align-items-center
                p Urutkan
                select.custom-select.custom-select-sm.border-0.shadow-sm.ml-2
                  option Paling Dicari
                  option Rating Terbaik
                  option Statistisi Terbaru
            h3 Jasa Olah Data
