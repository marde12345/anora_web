extends layout

block content
 .main-page.second.py-5
  .container
    .row
      .col-lg-4.left
        .profile_info
          .seller-card
            div
              .user-online-indicator.is-online(data-user-id='1152855')
                i.fa.fa-circle
                | online
            div
              a.ambassadors-badge(href='#') Statistisi
            .user-profile-info
              div
                .user-profile-image
                  label.user-pict
                    img.img-fluid.user-pict-img(src='/images/default_user.jpg' alt='Dieka Prastyas')
